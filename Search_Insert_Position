// Input: nums = [1,3,5,6], target = 5
// Output: 2

public class Solution {
    public int SearchInsert(int[] nums, int target) {
int index = -1;
for(int i = 0; i < nums.Length; i++)
{
    if (target <= nums[i])
    {
        index = i;
        break;
    }
        
}
return index < 0 ? nums.Length : index;
}
}
